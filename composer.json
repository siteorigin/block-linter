    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "classmap": ["block-linter.php"]
    },
    "bin": ["block-linter.php"],
    "scripts": {
        "test": "php block-linter.php -f test-blocks.html"
    }
}